{
  "name": "FlightDeck",
  "version": "0.1.0",
  "private": true,
  "main": "electron/electron.js",
  "homepage": "./",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.37.4",
    "@types/jest": "^24.0.18",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-easy-crop": "^1.13.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-select": "^3.0.4",
    "@types/reactstrap": "^8.0.4",
    "@types/redux-logger": "^3.0.7",
    "@types/shortid": "0.0.29",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "bootstrap": "^4.3.1",
    "check-dependencies": "^1.1.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "electron": "^6.0.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.5",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "font-awesome": "^4.7.0",
    "google_maps_js_api": "github:uas-at-ucla/google_maps_js_api#1.3",
    "image-clipper": "^0.4.4",
    "immer": "^3.2.0",
    "patch-package": "^6.1.2",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-easy-crop": "^1.15.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "react-select": "^3.0.4",
    "react-sortable-hoc": "^1.10.1",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0",
    "reselect-map": "^1.0.4",
    "sanitize.css": "^11.0.0",
    "shortid": "^2.2.14",
    "socket.io-client": "^2.2.0",
    "three": "^0.108.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2",
    "typescript-tslint-plugin": "^0.5.4",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "react-start": "cross-env PORT=3001 BROWSER=none react-scripts start",
    "electron-start": "npm run electron-build && cross-env ELECTRON_IS_DEV=1 electron .",
    "start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3001/ && npm run electron-start\"",
    "start-web": "react-scripts start",
    "start-prod": "cross-env ELECTRON_IS_DEV=0 electron .",
    "react-build": "react-scripts build",
    "electron-build": "tsc electron/electron.ts",
    "build": "npm run electron-build && npm run react-build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "package-mac": "npm run build && electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds  #--icon=assets/icons/mac/icon.icns",
    "package-win": "npm run build && electron-packager . Flight-Deck --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Flight Deck\"  #--icon=assets/icons/win/icon.ico",
    "package-linux": "npm run build && electron-packager . Flight-Deck --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds  #--icon=assets/icons/png/1024x1024.png",
    "postinstall": "patch-package"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
